{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build --dotenv ../.env.production",
    "dev": "nuxt dev --dotenv ../.env.development",
    "generate": "nuxt generate --dotenv ../.env.production",
    "preview": "nuxt preview --dotenv ../.env.production",
    "upgrade": "nuxi upgrade",
    "reset": "rm -rf .nuxt node_modules .output package-lock.json && bun install",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/types": "^2.17.3",
    "@nuxt/typescript-build": "^3.0.2",
    "@pinia/nuxt": "^0.5.1",
    "@vite-pwa/assets-generator": "^0.2.3",
    "@vite-pwa/nuxt": "^0.4.0",
    "nuxt": "^3.9.3",
    "nuxt-security": "^1.0.1",
    "pinia": "^2.1.7",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vuetify": "^3.5.0",
    "vite-plugin-vuetify": "^2.0.1"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^10.7.2"
  }
}
